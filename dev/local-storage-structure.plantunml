{
    "answeredCardsAll (list of answered questions)": {
        "1 (number of question)": ["0 (id of answered option)", "false (is correct?)", "true (is answered later?)"],
        "3": [1, true, "NaN"],
        "120": [3, false, "NaN"]    
    },
    "currentCardAll (Number of current card in 'all' mode)": 3,
    "currentAppMode (Current mode of app)": "all/error",
    "isRandomAll (is next question being randomly chosen in 'all' mode)": true  
}
@endjson

